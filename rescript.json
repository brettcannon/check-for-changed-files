{
  "name": "check-for-changed-files",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": ["@rescript/core"],
  "bs-dev-dependencies": ["@dusty-phillips/rescript-zora", "rescript-nodejs"],
  "bsc-flags": ["-open RescriptCore"]
}
